{
    "name": "beeyev/gettr-api-client-php",
    "description": "GETTR.com API PHP Client",
    "keywords": [
        "GETTR",
        "GETTR.com"
    ],
    "homepage": "https://github.com/beeyev/gettr-api-client-php",
    "authors": [
        {
            "name": "Alexander Tebiev",
            "email": "tebiev@mail.com",
            "homepage": "https://github.com/beeyev/",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4||^8.0",
        "guzzlehttp/guzzle": "^6.4||^7.0",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beeyev\\GettrApiClient\\Adapters\\Laravel\\GettrServiceProvider"
            ],
            "aliases": {
                "Gettr": "Beeyev\\GettrApiClient\\Adapters\\Laravel\\Facades\\GettrFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Beeyev\\GettrApiClient\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
